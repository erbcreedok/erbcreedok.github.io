#main
  height 725px
  background-image url("../images/bg.jpg")
  background-size auto 725px
  background-position-x center
  position: relative;

#main-text
  padding 9em 0px

#door-container
  position: absolute;
  left: 50%;
  width: 1px;
  bottom: 527px;
  margin-left: 290px;

#door-image
  width 190px
  position: absolute;

#door-shadow
  width: 361px;
  position: absolute;
  top: -50px;
  left: -80px;

#door-information
  position: absolute;
  width: 198px;
  left: -198px;
  top: 70px;
  color: white;
  font-size 21px
  p
    font-weight bold;
    margin-bottom 10px
  .underline
    width: 100%;
    height: 3px;
    background-color: white;
    margin-bottom: 10px;
  .square {
    border 2px solid white;
    width 24px
    height 24px
    float: left
    margin-right 8px
    text-align center
    line-height 18px
    cursor: pointer
    -webkit-transition: 200ms
    -moz-transition: 200ms
    -ms-transition: 200ms
    -o-transition: 200ms
    transition: 200ms
    color white
    &:hover {
      color black
      background-color white
    }
    i {
      display: inline-block
      margin-top -5px;
    }
  }
  .door-type {
    color #F7DDAD
  }

#door-number
  margin-left 12px
  float: left
  font-weight:normal
  line-height: 24px

#history
  margin-top -100px;



